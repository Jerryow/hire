            <strong>Copyright &copy; 2019-2021 <a href="https://www.mrmatch.net/" target="_blank">MrMatch.net</a>.</strong> All rights reserved.
        </footer>
    </div>
    <div class="modal fade" id="confirmModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    </div>

    <script id="modelHtml" type="text/x-jquery-tmpl">
        <div class="modal-dialog">
            <div class="modal-content" style="">
                <div class="modal-header bg-light-blue">
                    <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">关闭</span></button>
                    <h4 class="modal-title" id="myModalLabel">${$data.Title}</h4>
                </div>
                <div class="modal-body no-shadow" style="overflow:hidden;">
                    {{html $data.Body}}
                </div>
                <div class="modal-footer " style="background:#fff;">
                    {{if $data.Refresh.check}}
                    <button class="btn btn-default btn-sm pull-left btnRefresh" onclick="javascript: ${ $data.Refresh.function};"><i class="fas fa-redo"></i> 刷新</button>
                    {{/if}}
                    {{if $data.Submit.check}}
                    <button class="btn btn-success btnSubmit" onclick="javascript: ${ $data.Submit.function};">提交</button>
                    {{/if}}
                    {{if $data.Goback.check}}
                    <button class="btn btn-primary " data-dismiss="modal" onclick="javascript: ${ $data.Goback.function};">返回</button>
                    {{/if}}
                </div>
            </div>
        </div>
    </script>

    <div class="modal fade" id="deleteModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel2" aria-hidden="true">
