<body class="skin-blue  sidebar-mini">
    <!-- Site wrapper -->
    <div class="wrapper">
        @Html.Action("MainHeader", "PageCommon")
        @Html.Action("MainSidebar", "PageCommon")
        <div class="content-wrapper">

            @RenderBody()
        </div>
        <footer class="main-footer">
